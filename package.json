{
  "name": "ember-bootstrap-datepicker",
  "version": "1.0.0",
  "description": "Datepicker component for Ember CLI based on bootstrap-datepicker. It doesn't have any external dependency except bootstrap-datepicker, supports popup and inline modes.",
  "keywords": [
    "ember-addon",
    "ember-cli",
    "ember-datepicker",
    "bootstrap",
    "bootstrap-datepicker",
    "datepicker"
  ],
  "license": "MIT",
  "author": "Damian Senn <damian.senn@topaxi.codes>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/topaxi/ember-bootstrap-datepicker",
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "ember try:each"
  },
  "dependencies": {
    "bootstrap-datepicker": "^1.6.4",
    "broccoli-funnel": "^1.2.0",
    "broccoli-merge-trees": "^2.0.0",
    "broccoli-stew": "^1.5.0",
    "ember-cli-babel": "^6.0.0"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "ember-ajax": "^3.0.0",
    "ember-cli": "2.14.0-beta.2",
    "ember-cli-dependency-checker": "^1.3.0",
    "ember-cli-eslint": "^3.0.0",
    "ember-cli-htmlbars": "^1.1.1",
    "ember-cli-htmlbars-inline-precompile": "^0.4.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.0.0",
    "ember-cli-shims": "^1.1.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-resolver": "^4.0.0",
    "ember-source": "~2.13.0",
    "ember-welcome-page": "^3.0.0",
    "loader.js": "^4.2.3"
  },
  "engines": {
    "node": ">= 4"
  },
  "main": "index.js",
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "defaultBlueprint": "bootstrap-datepicker",
    "main": "index.js"
  }
}
